{
  "configurations": [
    {
      "name": "Dev Start",
      "request": "launch",
      "command": "npm run dev",
      "skipFiles": ["<node_internals>/**"],
      "type": "node-terminal"
    },
    {
      "name": "Attach by Process ID",
      "processId": "${command:PickProcess}",
      "request": "attach",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    },
    {
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "name": "nodemon",
      "program": "${workspaceFolder}/src/app.ts",
      "request": "launch",
      "restart": true,
      "runtimeExecutable": "/home/keyur_km/.nvm/versions/node/v18.12.0/bin/nodemon",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    }
  ]
}
